{
  "manifest_version": 3,
  "name": "Kite by Zerodha CSV Support",
  "version": "1.0",
  "description": "Add stock symbols from CSV in your Kite Watchlist",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.zerodha.com/*"],
      "js": ["content.js"]
    }
  ]
}
